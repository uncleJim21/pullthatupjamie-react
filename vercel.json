{
  "buildCommand": "CI=false npm run build",
  "rewrites": [
    {
      "source": "/researchSession/:shareId",
      "destination": "/api/researchSession/[shareId]"
    },
    {
      "source": "/blog/:slug",
      "destination": "/api/blog/[slug]"
    }
  ],
  "headers": [
    {
      "source": "/api/researchSession/:shareId",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=3600, stale-while-revalidate=86400"
        }
      ]
    },
    {
      "source": "/api/blog/:slug",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=600, stale-while-revalidate=120"
        }
      ]
    }
  ]
}
